.container
  #header.row
    .col-md-6
      h1 Todo App!
      if loggedIn
        a.btn.btn-xs.btn-danger href="#" click="logout" id="logout" Logout
    .col-md-6
      if loggedIn
        br
        ul
          li: small Use the API by providing the keys "user_email" and  "user_token" with the values displayed below on each request
          li 
            strong email: 
            = user.email
          li
            strong token: 
            = user.authentication_token
                    
  #content.row= outlet